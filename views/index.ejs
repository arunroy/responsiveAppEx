	<div class="hidden-sm-up" id="prodInfo">
		<div class="row" >
			<div class="col-sm-3 data-row data-row-style-1" >{{currentItem.product}}</div>
			<div class="col-sm-3 data-row data-row-style-2">{{currentItem.interest_rate}}</div>
			<div class="col-sm-3 data-row data-row-style-1">{{currentItem.minimum_deposit}}</div>
			<div class="col-sm-3 data-row data-row-style-2">{{currentItem.interest_type}}</div>
		</div>
		<div>
			<a ng-click="loadPrevItem()" href="#" id="prevBtn" class="pull-left">&lt;{{prevItem.product}}</a>	
			<a ng-click="loadNextItem()" href="#" id="nextBtn" class="pull-right">{{nextItem.product}}&gt;</a>
		</div>

		<div class="row">
			<div class="col-xs-6 align-left"></div>
			
			<div class="col-xs-6 align-right"></div>
		</div>
	</div>
	<div class="row hidden-sm-down">
		<div class="col-md-5">
			<div class="table-responsive">
				<table class="table table-bordered table-striped">
				  <thead>
					<tr>
					  <th>Product</th>
					  <th>Interest Rate</th>
					  <th>Min Deposit</th>
					  <th>Interest Type</th>
					</tr>
				  </thead>
				  <tbody class="">
					<tr ng-repeat="item in userInfo.data" class="">
					  <td>{{item.product}}</td>
					  <td>{{item.interest_rate}}</td>
					  <td>{{item.minimum_deposit}}</td> 
					  <td>{{item.interest_type}}</td>
					</tr>
				  </tbody>
				</table>
			</div>
   		</div>
   		<div class="col-md-7"></div>
    </div>
</div>
<div ng-controller="searchUserCtrl">
	<div>
	  <h1>Search User</h1>
	  <div class="input-group">
      	<input type="text" class="form-control" placeholder="Search user...">
      </div>
	</div>
	
		<table class="table table-striped table-bordered">
			<thead>
				<th>Name</th>
				<th  class="hidden-xs-down">Age</th>
				<th  class="hidden-xs-down">Shoe size</th>
				<th>Title</th>
			</thead>
			<tr ng-repeat="item in employee" >
				<td>{{item.name}}</td>
				<td class="hidden-xs-down">{{item.age}}</td>
				<td class="hidden-xs-down">{{item.shoe_size}}</td>
				<td>{{item.title}}</td>
			</tr>
		</table>
</div>
